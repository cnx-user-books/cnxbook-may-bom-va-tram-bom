                                    <m:mi>a</m:mi>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
                              <m:mo stretchy="false">−</m:mo>
                              <m:msub>
                                <m:mstyle fontsize="24pt">
                                  <m:mrow>
                                    <m:mi>p</m:mi>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mstyle fontstyle="italic">
                                      <m:mrow>
                                        <m:mtext>hh</m:mtext>
                                      </m:mrow>
                                    </m:mstyle>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
                            </m:mrow>
                            <m:mi>g</m:mi>
                          </m:mfrac>
                          <m:mo stretchy="false">−</m:mo>
                          <m:mfrac>
                            <m:msubsup>
                              <m:mstyle fontsize="24pt">
                                <m:mrow>
                                  <m:mi>C</m:mi>
                                </m:mrow>
                              </m:mstyle>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mn>1</m:mn>
                                </m:mrow>
                              </m:mstyle>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:mstyle>
                            </m:msubsup>
                            <m:mn>2g</m:mn>
                          </m:mfrac>
                        </m:mrow>
                        <m:mo stretchy="false">−</m:mo>
                        <m:mi fontstyle="italic">Dh</m:mi>
                      </m:mrow>
