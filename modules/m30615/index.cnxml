                  </m:mrow>
                </m:mrow>
              </m:mstyle>
              <m:mrow/>
            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{ \[  { size 24{H} }  rSub { size 8{ ital "ck"} }  \] = {  { { size 24{p} }  rSub { size 8{a} } - { size 24{p} }  rSub { size 8{ ital "hh"} } }  over  {g} } - {  { { size 24{C} }  rSub { size 8{1} }  rSup { size 8{2} } }  over  {2g} } -Dh} {}</m:annotation>
