                      <m:mo stretchy="false">]</m:mo>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mrow>
                              <m:msub>
                                <m:mstyle fontsize="24pt">
                                  <m:mrow>
                                    <m:mi>p</m:mi>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mstyle fontsize="8pt">
